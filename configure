                   int ok;
                  #else
                   error fail
                  #endif

_ACEOF
if ac_fn_c_try_compile "$LINENO"
then :
  gl_cv_host_cpu_c_abi_32bit=no
else case e in #(
  e) gl_cv_host_cpu_c_abi_32bit=yes ;;
