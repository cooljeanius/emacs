   when using C++. */
#ifdef __cplusplus
extern "C" void ucatch(int dummy) { }
#else
void ucatch(dummy) int dummy; { }
#endif /* __cplusplus */

int main(void)
{
  int i = fork(), status;

  if (i == 0)
    {
      sleep(3);
      kill(getppid(), SIGINT);
      sleep(3);
      return 0;
    }

  signal(SIGINT, ucatch);

  status = wait(&i);
  if (status == -1)
    wait(&i);

  return status == -1;
}
